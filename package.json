{
  "name": "rebel-wallet",
  "version": "1.4.0",
  "description": "Web wallet for Ravencoin",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build index.html --no-source-maps",
