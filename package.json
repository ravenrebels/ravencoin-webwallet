    "@ravenrebels/ravencoin-key": "^2.5.2",
    "@ravenrebels/ravencoin-message": "^0.7.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "crypto-js": "^4.2.0",
    "parcel": "^2.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-reader": "^3.0.0-beta-1"
  },
  "overrides": {
    "react-qr-reader": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.13.0",
